{
  "ts-node": {
    "compilerOptions": {
      "module": "commonjs"
    }
  },
  "compilerOptions": {
    "target": "ES6",                                 
    "module": "ES2022",
    "moduleResolution": "Node16",
    "esModuleInterop": false,                            
    "forceConsistentCasingInFileNames": true,          
    "strict": true,                                               
    "skipLibCheck": true                               
  }
}
